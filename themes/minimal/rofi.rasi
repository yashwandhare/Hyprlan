* {
    /* Solid Base */
    bg-solid:    #1a1a1a;
    
    /* Glass Base (90%) */
    bg-glass:    #1a1a1aE6;
    
    /* Default to Glass if not overridden */
    bg:          @bg-glass;
    
    fg:          #E3E6E8;
    accent:      #A2B6C9;
    accent-dim:  #A2B6C926;
    border:      #A2B6C933;
    disabled:    #7F8490;
    urgent:      #d99a9a;
}

